{
  "compilerOptions": {
    "lib": ["es5", "es6"],
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "node",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "noEmitOnError": false,

    "noImplicitAny": false,
    "strict": false,
    "esModuleInterop": true,
   
  }
}
