{
  "compilerOptions": {
    "lib": ["es2020"],
    "target": "es2020",
    "module": "es2020",
  //  "allowImportingTsExtensions": true,
    "moduleResolution": "bundler",
   
    //  "allowImportingTsExtensions": true,
    "outDir": "./dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "noEmit": false,
    "noImplicitAny": false,
    "strict": false,
    "esModuleInterop": true,
  
   
  }
}
